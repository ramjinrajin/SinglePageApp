    <title>@ViewBag.Title</title>
    <!-- Bootstrap Core CSS -->

    <script src="~/Script/jquery-3.0.0.js"></script>
    <link href="~/Style/bootstrap.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/Style/1-col-portfolio.css" rel="stylesheet">
